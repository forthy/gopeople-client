{
  "name": "go-people-client",
  "version": "1.0.0",
  "description": "Provides a client to Austria's GoPeople service.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "test": "jest --config=jestconfig.json",
    "test:single": "jest --config jestconfig.json --detectOpenHandles -i src/__tests__/GoPeopleClient.test.ts -t 'validateTime function should function properly'",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(ts|js)\"",
    "lint": "tslint -p tsconfig.json",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test:coverage": "jest --config=jestconfig.json --coverage",
    "typedoc": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forthy/go-people-client"
  },
  "homepage": "https://github.com/forthy/go-people-client#readme",
  "bug": {
    "url": "https://github.com/forthy/go-people-client/issues"
  },
  "keywords": [
    "GoPeople",
    "client",
    "typescript"
  ],
  "author": "Richard Chuo",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.3",
    "jest": "^26.1.0",
    "mockttp": "^0.20.3",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.5"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@microsoft/tsdoc": "^0.12.20",
    "@types/faker": "^4.1.12",
    "@types/lodash": "^4.14.157",
    "@types/luxon": "^1.24.1",
    "@types/request-promise-native": "^1.0.17",
    "dotenv": "^8.2.0",
    "faker": "^4.1.0",
    "fp-ts": "^2.6.7",
    "fp-ts-contrib": "^0.1.17",
    "lodash": "^4.17.15",
    "luxon": "^1.24.1",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8"
  }
}